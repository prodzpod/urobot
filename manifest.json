{
    "manifest_version": 2,
    "name": "yumewikiToConnectionsData",
    "version": "1.0",
  
    "description": "Copies connection points from yumewiki to urobot content",
  
    "permissions": [
        "activeTab",
        "clipboardRead",
        "clipboardWrite"
      ],

    "content_scripts": [
      {
        "matches": ["*://yume.wiki/*"],
        "js": ["copy.js"]
      }
    ]
  }
  